<h1>New User Registration</h1>

<%= form_for @user do |f| %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br />

  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br />

  <%= f.label :email %>
  <%= f.email_field :email %><br />

  <%= f.label :phone %>
  <%= f.phone_field :phone %><br />

  <%= f.submit %>

<% end %>